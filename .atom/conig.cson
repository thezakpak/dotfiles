  "linter-phpmd":
    executablePath: "/usr/local/bin/anyenv/envs/phpenv/shims/phpmd"
  "linter-ui-default":
    panelHeight: 300
  minimap:
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      "minimap-autohide": true
      "minimap-autohideDecorationsZIndex": 0
  nuclide:
    "nuclide-working-sets": {}
    use:
      "atom-ide-diagnostics-ui": false
  "php-integrator-base":
    linting: {}
  "platformio-ide-terminal":
    customTexts: {}
    style:
      theme: "solarized-dark"
  "symbols-tree-view":
    autoHide: true
    autoToggle: true
  "tree-view":
    alwaysOpenExisting: true
    autoReveal: true
  welcome:
    showOnStartup: false
".blade.html.php.text":
  editor:
    commentEnd: " --}}"
    commentStart: "{{-- "
