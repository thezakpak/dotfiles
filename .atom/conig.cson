  "atom-clock":
    dateFormat: "DD/MM/YYYY H:mm"
    locale: "ja"
  "atom-ide-ui":
    "atom-ide-code-format": {}
    hideNuclideWarning: true
    hyperclick:
      darwinTriggerKeys: "ctrlKey"
    use: {}
  "atom-material-ui":
    colors:
      abaseColor: "#2684f1"
      predefinedColor: "Blue"
    tabs:
      compactTabs: true
      stretchedTabs: true
    treeView:
      compactList: true
    ui:
      animations: false
      panelContrast: true
  "atom-package-deps":
    ignored: [
      "linter-ui-default"
    ]
  core:
    allowPendingPaneItems: false
    automaticallyUpdate: false
    closeEmptyWindows: false
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".hgrc"
      ]
      "source.json": [
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    destroyEmptyPanes: false
    disabledPackages: [
      "linter"
      "github"
      "git-diff"
    ]
    followSymlinks: false
    packagesWithKeymapsDisabled: [
      "platformio-ide-terminal"
      "tidalcycles"
    ]
    telemetryConsent: "no"
    themes: [
      "atom-material-ui"
      "solarized-dark-syntax"
    ]
  "disable-keybindings": {}
  docblockr: {}
  editor:
    fontSize: 9
    scrollPastEnd: true
    showIndentGuide: true
  "exception-reporting":
    userId: "09793c87-d004-40e1-acc2-2e097e737db5"
  "ide-php":
    phpPath: "/usr/local/bin/anyenv/envs/phpenv/shims/php"
  "linter-php":
    executablePath: "/usr/local/bin/anyenv/envs/phpenv/shims/php"
  "linter-phpcs":
